{
  "routes": [
    {
      "route": "/",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/dashboard/*",
      "allowedRoles": ["authenticated"],
      "serve": "/index.html"
    },
    {
      "route": "/admin/*",
      "allowedRoles": ["admin"],
      "serve": "/index.html"
    },
    {
      "route": "/auth/*",
      "allowedRoles": ["anonymous"],
      "serve": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/images/*", "/*.{css,scss,js,png,gif,ico,jpg,svg,woff,woff2,ttf,eot}"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".css": "text/css"
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://accounts.google.com; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; connect-src 'self' https://stealth-tinko-prod-app-1762804410.azurewebsites.net https://accounts.google.com; font-src 'self' data:;"
  },
  "responseOverrides": {
    "404": {
      "redirect": "/",
      "statusCode": 302
    }
  }
}